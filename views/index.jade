extends layout

block header
  script(src='/socket.io/socket.io.js')

block content
  h1= title
  p Welcome to #{title}
  ul
  each user in users
    li
      = user.username

  script(type='text/javascript').
    var socket = io.connect('http://localhost');
    socket.on('news', function (data) {
      console.log(data);
      socket.emit('my other event', { my: 'data' });
    });